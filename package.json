{
  "name": "citation-app",
  "license": "MIT",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "jest",
    "postinstall": "husky install",
    "lint": "eslint --ignore-path .eslintignore",
    "prettier": "prettier --ignore-path .eslintignore --write \"**/*.{ts,tsx,css,scss,vue}\"",
    "validate": "npm-run-all --parallel check-format lint",
    "format": "npm run prettier && npm run lint && git add -A ."
  },
  "dependencies": {
    "axios": "^1.3.6",
    "dotenv": "^16.0.3",
    "moment": "^2.29.4",
    "vue": "^3.2.47",
    "vue-router": "4",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@types/node": "^18.15.13",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.11.0",
    "http-proxy-middleware": "^2.0.6",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.1",
    "pinst": "^3.0.0",
    "postcss": "^8.4.23",
    "prettier": "^2.8.7",
    "pretty-quick": "^3.1.3",
    "tailwindcss": "^3.3.1",
    "typescript": "^5.0.2",
    "vite": "^4.3.0",
    "vue-tsc": "^1.2.0"
  }
}