{
  "name": "citation-app",
  "license": "MIT",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "lint": "eslint . --ext .ts,.vue",
    "prettier": "prettier --write \"src/**/*.{vue,js,jsx,ts,tsx}\"",
    "test": "jest",
    "lint-staged": "lint-staged",
    "pre-commit" : "yarn prettier git add ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn pre-commit",
      "pre-push": "yarn run test"
    }
  },
  "dependencies": {
    "axios": "^1.3.6",
    "dotenv": "^16.0.3",
    "vue": "^3.2.47",
    "vue-router": "4",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@types/node": "^18.15.13",
    "@vitejs/plugin-vue": "^4.1.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.39.0",
    "http-proxy-middleware": "^2.0.6",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.1",
    "postcss": "^8.4.23",
    "prettier": "^2.8.7",
    "pretty-quick": "^3.1.3",
    "tailwindcss": "^3.3.1",
    "typescript": "^5.0.2",
    "vite": "^4.3.0",
    "vue-tsc": "^1.2.0"
  }
}
